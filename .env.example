# NexNews example environment variables
# Database
DATABASE_URL=sqlite:///./data/news.db

# App
APP_NAME=NexNews
ENV=dev
PORT=8000

# SMTP (configure only if you want real emails sent)
EMAIL_ENABLED=false
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASSWORD=your-smtp-password
SMTP_STARTTLS=true
SMTP_FROM=noreply@nexnews.com
